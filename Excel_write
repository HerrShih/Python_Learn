<script type="text/javascript" class="ng-scope">
var url = contextPath+'/op/appCase/OpAppCase/photoInfo.do';
var param = {"caseId":caseId};
$.ajax({
	type:'post',
	data: param,
	url:url,
	success:function(data){
		if(data.userPhoto != null){
			if(data.userPhoto.BACK != null && data.userPhoto.FRONT != null && data.userPhoto.FACE != null){
				loanImage(data.userPhoto.BACK,data.userPhoto.FRONT,data.userPhoto.FACE,data.userPhoto.POLICE);
			} else {
				$("#imgTitle").hide();
				$("#imgTable").hide();
			}
		} else {
			$("#imgTitle").hide();
			$("#imgTable").hide();
		}
	}
});
function loanImage(userPhotoBACK,userPhotoFRONT,userPhotoFACE,userPhotoPOLICE){
	// 影像图片
	var userPhoto_BACK_path = userPhotoBACK.path;
	var userPhoto_FRONT_path = userPhotoFRONT.path;
	var userPhoto_FACE_path = userPhotoFACE.path;
    var frontIviewer;
    var backIviewer;
    var faceIviewer;
    var policeIviewer;
	if (userPhoto_FRONT_path != null) {
		 var frontUrl = contextPath + '/wbps/business/CaseAttachment/findAttachment.do?attachmentFullPath=' + userPhoto_FRONT_path + '&caseId=' + caseId + '&imageType=ID_CARD';
		 if (frontIviewer) {
			frontIviewer.iviewer('loadImage', frontUrl);
		 } else {
		 	frontIviewer = $("#frontImgBox").iviewer({src: frontUrl});
		 }
	 }
	 if (userPhoto_BACK_path != null) {
		 var backUrl = contextPath + '/wbps/business/CaseAttachment/findAttachment.do?attachmentFullPath=' + userPhoto_BACK_path + '&caseId=' + caseId + '&imageType=ID_CARD';
		 if (backIviewer) {
		 	backIviewer.iviewer('loadImage', backUrl);
		 } else {
		 	backIviewer = $("#backImgBox").iviewer({src: backUrl});
		 }
	 }
	 if (userPhoto_FACE_path != null) {
		 var faceUrl = contextPath + '/wbps/business/CaseAttachment/findAttachment.do?attachmentFullPath=' + userPhoto_FACE_path + '&caseId=' + caseId + '&imageType=ID_CARD';
		 if (faceIviewer) {
		 	faceIviewer.iviewer('loadImage', faceUrl);
		 } else {
		 	faceIviewer = $("#faceImgBox").iviewer({src: faceUrl});
		 }
	 }
	 if (userPhotoPOLICE != null && userPhotoPOLICE.path != null) {
		 var policeUrl = contextPath + '/wbps/business/CaseAttachment/findAttachment.do?attachmentFullPath=' + userPhotoPOLICE.path + '&caseId=' + caseId + '&imageType=ID_CARD';
		 if (policeIviewer) {
			 policeIviewer.iviewer('loadImage', policeUrl);
		 } else {
			 policeIviewer = $("#policeImgBox").iviewer({src: policeUrl});
		 }
	 } else {
		 $("#policeImgBox").html("无照片");
	 }
	 
	 // 影像信息
	 $("#backTable").append("<tr><td>ID Photo（正式身份证照片）分值："+userPhotoBACK.idPhoto+"</td><td>confidence 分值："+userPhotoBACK.confidence+"</td></tr>")
	 .append("<tr><td>Temporary ID Photo（临时身份证照片）分值："+userPhotoBACK.temporaryIDPhoto+"</td><td>1e-3 分值："+userPhotoBACK.le3+"</td></tr>")
	 .append("<tr><td>Photocopy（正式身份证的复印件） 分值："+userPhotoBACK.photocopy+"</td><td>1e-4 分值："+userPhotoBACK.le4+"</td></tr>")
	 .append("<tr><td>Screen（手机或电脑截屏翻拍的照片）分值："+userPhotoBACK.screen+"</td><td>1e-5 分值："+userPhotoBACK.le5+"</td></tr>")
	 .append("<tr><td>Edited（用工具合成或者编辑过的身份证图片）分值："+userPhotoBACK.edited+"&nbsp;&nbsp;</td><td>1e-6 分值："+userPhotoBACK.le6+"</td></tr>");
	
	 $("#frontTable").append("<tr><td>ID Photo（正式身份证照片）分值："+userPhotoFRONT.idPhoto+"</td><td>confidence 分值："+userPhotoFRONT.confidence+"</td></tr>")
	 .append("<tr><td>Temporary ID Photo（临时身份证照片）分值："+userPhotoFRONT.temporaryIDPhoto+"</td><td>1e-3 分值："+userPhotoFRONT.le3+"</td></tr>")
	 .append("<tr><td>Photocopy（正式身份证的复印件） 分值："+userPhotoFRONT.photocopy+"</td><td>1e-4 分值："+userPhotoFRONT.le4+"</td></tr>")
	 .append("<tr><td>Screen（手机或电脑截屏翻拍的照片）分值："+userPhotoFRONT.screen+"</td><td>1e-5 分值："+userPhotoFRONT.le5+"</td></tr>")
	 .append("<tr><td>Edited（用工具合成或者编辑过的身份证图片）分值："+userPhotoFRONT.edited+"&nbsp;&nbsp;</td><td>1e-6 分值："+userPhotoFRONT.le6+"</td></tr>");

	 $("#faceTable").append("<tr><td>ID Photo（正式身份证照片）分值："+userPhotoFACE.idPhoto+"</td><td>confidence 分值："+userPhotoFACE.confidence+"</td></tr>")
	 .append("<tr><td>Temporary ID Photo（临时身份证照片）分值："+userPhotoFACE.temporaryIDPhoto+"</td><td>1e-3 分值："+userPhotoFACE.le3+"</td></tr>")
	 .append("<tr><td>Photocopy（正式身份证的复印件） 分值："+userPhotoFACE.photocopy+"</td><td>1e-4 分值："+userPhotoFACE.le4+"</td></tr>")
	 .append("<tr><td>Screen（手机或电脑截屏翻拍的照片）分值："+userPhotoFACE.screen+"</td><td>1e-5 分值："+userPhotoFACE.le5+"</td></tr>")
	 .append("<tr><td>Edited（用工具合成或者编辑过的身份证图片）分值："+userPhotoFACE.edited+"&nbsp;&nbsp;</td><td>1e-6 分值："+userPhotoFACE.le6+"</td></tr>");
 }
</script>
